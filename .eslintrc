{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "plugins": [
    "react",
    "import",
    "jest",
  ],
  "rules": {
    "max-len": [1, 120, 2, {"ignoreComments": true}],
    "prop-types": [2]
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "node": true,
    "jest/globals": true
  },
}
